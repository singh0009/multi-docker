{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "singh0009/multi-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "singh0009/multi-server",
            "hostname": "api",
            "essential":false,
            "memory": 128
        },
        {
            "name":"worker",
            "image":"singh0009/multi-worker",
            "hostname":"worker",
            "essential":false,
            "memory": 128
        },
        {
            "name":"nginx",
            "image":"singh0009/multi-nginx",
            "hostanme":"nginx",
            "essential":true,
            "portMapping": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links":["client", "server"],
            "memory": 128
        }
    ]
}